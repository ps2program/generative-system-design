digraph "ER Schema" {
	rankdir=LR size="8,5"
	components [label="[components]
- id [PK]
- name
- description"]
	requirements [label="[requirements]
- id [PK]
- component_id [FK]
- data [JSON]"]
	functions [label="[functions]
- id [PK]
- data [JSON]"]
	physicals [label="[physicals]
- id [PK]
- data [JSON]"]
	requirement_function [label="[requirement_function]
- requirement_id [FK]
- function_id [FK]"]
	function_physical [label="[function_physical]
- function_id [FK]
- physical_id [FK]"]
	sub_requirement [label="[sub_requirement]
- parent_id [FK]
- child_id [FK]"]
	sub_function [label="[sub_function]
- parent_id [FK]
- child_id [FK]"]
	sub_physical [label="[sub_physical]
- parent_id [FK]
- child_id [FK]"]
	components -> requirements [label="has many" arrowhead=crow arrowsize=1.2]
	requirements -> functions [label="maps to" arrowhead=crow arrowsize=1.2 dir=both]
	functions -> physicals [label="maps to" arrowhead=crow arrowsize=1.2 dir=both]
	requirements -> requirements [label="sub-requirements" arrowhead=crow arrowsize=1.2 dir=both]
	functions -> functions [label="sub-functions" arrowhead=crow arrowsize=1.2 dir=both]
	physicals -> physicals [label="sub-physicals" arrowhead=crow arrowsize=1.2 dir=both]
}
